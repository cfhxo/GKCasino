# Database Connection
MONGO_URI=mongodb://[username]:[password]@[host]:[port]/[database]

# Security
JWT_SECRET=your_jwt_secret_at_least_32_characters_long

# Server Configuration
PORT=5000
NODE_ENV=development

# Authentication Keys
PASSWORD_KEY=your_password_encryption_key

# Google OAuth
WEB_GOOGLE_CLIENT_ID=your_web_client_id.apps.googleusercontent.com
IOS_GOOGLE_CLIENT_ID=your_ios_client_id.apps.googleusercontent.com
IOS_GOOGLE_REDIRECT_URI=com.googleusercontent.apps.your_ios_client_id:/

# Payment Processing
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret
PAYPAL_MODE=sandbox_or_live

# Email Configuration
MAILCOW_USER=your_email_username
MAILCOW_PASS=your_email_password
MAILCOW_FROM=your_sender_email_address

# Frontend URL
FRONTEND_URL=https://your-frontend-domain.com